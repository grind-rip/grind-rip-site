body{font-family:fira mono,monospace,-apple-system;font-size:16px;line-height:1.5rem}h1,h2,h3,h4,h5,h6{font-size:1rem;font-weight:600;margin:1.5rem 0 0}h2,h3,h4,h5,h6{font-weight:400}h1:before{content:"# "}h2:before{content:"## "}h3:before{content:"### "}h4:before{content:"#### "}h5:before{content:"##### "}h6:before{content:"###### "}h1:before,h2:before,h3:before,h4:before,h5:before,h6:before{color:#5b6268}h1:first-child{margin-top:0}p{margin:0 0 1.5rem}a:link,a:visited{color:#5699af}a:hover,a:active,a.active{color:#46d9ff}ul{margin:0 0 1.5rem;padding-left:1.25rem}ol{margin:0 0 1.5rem;padding-left:1.75rem}ul ul,ul ol,ol ul,ol ol{margin:0}ul li::marker{content:'*\00A0';color:#5b6268}ol li::marker{color:#5b6268}dt{font-weight:700;margin:0}dd{font-style:italic;margin:0 0 0 1.5rem}dd+dt{margin-top:1.5rem}dl{margin:0 0 1.5rem}blockquote{position:relative;margin:0 0 1.5rem 1.5rem}blockquote::before{position:absolute;left:-1.5rem;content:">";color:#5b6268}code,kbd,pre,samp{font-family:fira mono,monospace,-apple-system;background-color:#21242b;color:#5b6268}pre{overflow-x:auto;margin:0 0 1.5rem;padding:1.5rem}.highlight div{overflow-x:auto}b,strong{font-weight:600}::selection,mark{background-color:#bbc2cf;color:#282c34}hr{border:0;margin-bottom:1rem;margin-top:1rem}hr:after{content:'---'}sub,sup{font-size:unset;position:relative;top:-.25rem;vertical-align:baseline}sub{top:.25rem}table{border-spacing:0;margin:0 0 1.5rem;overflow-wrap:anywhere}th,td{padding:0 .75rem;vertical-align:top}th:first-child,td:first-child{padding-left:0}th{text-align:inherit}img{height:auto;max-width:100%}.logo{display:block;background-color:#21242b;color:#bbc2cf;font-weight:inherit;margin:0;padding:.25rem}.logo:before{content:none}a.logo-inner:link,a.logo-inner:visited{color:inherit;text-decoration:inherit}a.logo-inner:hover,a.logo-inner:active,a.logo-inner.active{color:#ff6c6b}.logo:before{content:"[";color:#bbc2cf}.logo:after{content:"] $";color:#bbc2cf}.aside ul{display:flex;flex-flow:column wrap;gap:0 1.5rem;justify-content:flex-start;margin:0;padding:0 0 .25rem}.aside li{margin-left:1rem;padding-top:.25rem;text-transform:lowercase}.aside li::marker{content:"./";color:#5b6268}.footer{background-color:#282c34;color:#5b6268;margin-top:1rem}.footer p{margin:0;padding:.25rem}.page{display:grid;max-width:68rem;grid-row-gap:2rem;grid-template-areas:"header" "body" "aside" "footer";grid-template-columns:minmax(0,1fr);margin:1rem auto}.page .header{grid-area:header;display:flex}.page .logo{flex-shrink:0}.page .nav{flex-grow:1}.page .body{grid-area:body;overflow-wrap:break-word}.page .aside{grid-area:aside}.page .footer{grid-area:footer}@media(min-width:45rem){.page{grid-template-areas:"header  header" "body    aside" "footer  aside";grid-template-columns:minmax(0,1fr)15rem;grid-column-gap:2rem}}.nav ul{display:flex;flex-flow:row wrap;gap:0 1.5rem;justify-content:flex-start;margin:0;padding:0 0 .25rem}.nav li{margin-left:1rem;padding-top:.25rem;text-transform:lowercase}.nav li::marker{content:"./";color:#5b6268}.about{height:1.5rem}.about{display:inline;vertical-align:top}.about::before{content:none}.aside .socials{font-size:20px;padding:0;height:1.5rem}.aside .socials li{display:inline-block;margin:0}.aside .socials li::marker{content:none}img{display:block;margin-left:auto;margin-right:auto;max-height:65%;max-width:65%}.poem{font-style:italic;text-align:center}body{background-color:#282c34;color:#bbc2cf}